{
  "Manifest Template": {
    "prefix": ["ifmanifest", "manifest"],
    "body": [
      "name: $1",
      "description: $2",
      "tags: $3",
      "initialize:",
      "  plugins:",
      "    ${4:<PLUGIN-NAME-HERE>}:",
      "      method: $5",
      "      path: $6",
      "  outputs:",
      "tree:",
      "  children:",
      "    child:",
      "      pipeline:",
      "      config:",
      "      defaults:",
      "      inputs:",
      "        - timestamp: $7",
      "          duration: $8"
    ],
    "description": "Creates a new manifest template"
  },
  "Manifest Initialize Section": {
    "prefix": "initialize",
    "body": [
      "initialize:",
      "  plugins:",
      "    ${1:<PLUGIN-NAME-HERE>}:",
      "      method: ${2:<METHOD-HERE>}",
      "      path: ${3:<PATH-HERE>}",
      "  outputs:"
    ],
    "description": "Impact Framework manifest \"Initialize\" section template."
  },
  "Manifest Child Node": {
    "prefix": "child",
    "body": [
      "child:",
      "  pipeline:",
      "  config:",
      "  defaults:",
      "  inputs:",
      "    - timestamp: ${1:<TIMESTAMP-VALUE>}:",
      "      duration: ${2:<DURATION-VALUE>}"
    ],
    "description": "Impact Framework manifest \"child\" template."
  }
}
